{% extends "front/base.html.twig" %}

{% block title %}{% trans %}MY PROFILE{% endtrans %}{% endblock %}

{% block content_title %}{% trans %}MY PROFILE{% endtrans %}{% endblock %}

{% block content %}
    <div class="pure-g">
        <div class="pure-u-1-4"></div>
        <div class="pure-u-1-2 seller-form">
            <ul>
                {% for immo in list %}
                    <li>
                        <a href="{{ path('app_sell_editrealestatestep',{pk: immo.pk, step:1}) }}">Bien immobilier  {{ immo.address }} {{ immo.city }}</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>            
{% endblock %}